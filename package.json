{
  "name": "dive",
  "version": "0.0.1",
  "description": "Yuka showcase",
  "repository": "Mugen87/dive",
  "scripts": {
    "build": "rollup -c \"config/rollup/config.js\"",
    "build-prod": "rollup -c \"config/rollup/config.js\" && google-closure-compiler build/bundle.js > build/bundle.min.js",
    "dev": "concurrently \"rollup -c \"config/rollup/config.js\" -w\" \"serve\"",
    "lint": "eslint src --ext .js",
    "start": "npm run dev"
  },
  "eslintConfig": {
    "extends": "mdcs"
  },
  "author": "Mugen87",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-mdcs": "^4.2.3",
    "google-closure-compiler": "^20190929.0.0",
    "rollup": "^2.3.0",
    "serve": "^11.3.0"
  },
  "dependencies": {
    "dat.gui": "0.7.6",
    "three": "0.113.0",
    "yuka": "0.3.0"
  }
}
